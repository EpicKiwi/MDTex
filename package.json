  "version": "1.1.0",
  "author": "EpicKiwi <me@epickiwi.fr>",
  "bin": {
    "mdtex": "./bin/run"
  },
  "bugs": "https://github.com/EpicKiwi/mdtex/issues",
  "dependencies": {
    "@oclif/command": "^1.5.8",
    "@oclif/config": "^1.10.4",
    "@oclif/plugin-help": "^2.1.4",
    "chalk": "^2.4.2",
    "fastparse": "^1.1.2",
    "js-yaml": "^3.12.1",
    "latex": "0.0.1",
    "lodash": "^4.17.11",
    "mustache": "^3.0.1",
    "rebber": "^3.1.1",
    "remark-parse": "^6.0.3",
    "unified": "^7.1.0",
    "unist-util-visit": "^1.4.0",
    "vfile": "^4.0.0",
    "watchpack": "^1.6.0",
    "glob": "latest"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.21.0",
    "globby": "^8.0.1",
    "prettier": "^1.15.3"
